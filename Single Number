class Solution:
    # @param {integer[]} nums
    # @return {integer}
    def singleNumber(self, nums):
        if len(nums)==1:
            return nums[0]
        nums.sort()
        i=0
        while nums[i]==nums[i+1]:
            i+=2
            if i==len(nums)-1:
                return nums[-1]
        return nums[i]
