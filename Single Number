class Solution:
    # @param {integer[]} nums
    # @return {integer}
    
# XOR method
    def singleNumber(self, nums):
        ans=0
        for i in nums:
            ans^=i
        return ans
# Sort
    '''
    def singleNumber(self, nums):
        if len(nums)==1:
            return nums[0]
        nums.sort()
        i=0
        while nums[i]==nums[i+1]:
            i+=2
            if i==len(nums)-1:
                return nums[-1]
        return nums[i]
    ''' 
