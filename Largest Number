class Solution:
    # @param {integer[]} nums
    # @return {string}
## compair every two elements with string adding ,'a'+'b' and 'b'+'a'
    def largestNumber(self, nums):
        if set(nums)==set([0]):
            return '0'
        numss=map(str,nums)
        for i in range(len(numss)):
            for j in range(i,len(numss)):
                if numss[i]+numss[j]<numss[j]+numss[i]:
                    numss[i],numss[j]=numss[j],numss[i]
        return ''.join(numss)
            
